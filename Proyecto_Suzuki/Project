        <%  
            String pidus=request.getParameter("pidus");
            String pus=request.getParameter("pus");
            String pcl=request.getParameter("pcl");
            String ptus=request.getParameter("ptus");
            
           
            
       if(request.getParameter("modificar")!=null)
       {
       String Id_usuario=request.getParameter("txtidus");
       String Usuario=request.getParameter("txtusu");
       String Clave=request.getParameter("txtcl");
       String TipoUsuario=request.getParameter("txttus");
       
       
       
       try
      {
        bdSegura dbs=new bdSegura();
        Connection conex=dbs.conSeg();
       PreparedStatement pstmt=null;
       int correcto=0;
       String consulta="UPDATE usuarios set Id_usuario=?,Usuario=?,Clave=?,TipoUsuario=? where Id_usuario like ? ";
        
       
       pstmt=conex.prepareStatement(consulta);

pstmt.setString(1,Id_usuario);
pstmt.setString(2,Usuario);
pstmt.setString(3,Clave);
pstmt.setString(4,TipoUsuario);


correcto=pstmt.executeUpdate();

if(correcto>=1)
{
%>

