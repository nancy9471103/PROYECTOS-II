    <label for="exampleInputEmail1" class="form-label">Precio</label>
    <input type="text" name="txtprecio" id="nombre" class="form-control" required="required" placeholder="Ingrese precio de la motocicleta" autocomplete="off" value="<%=precio%>" >
  </div>  <BR>  
  

   <div class="col-xs-4">
