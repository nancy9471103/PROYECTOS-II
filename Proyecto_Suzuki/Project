       String Id_usuario,Usuario,Clave,TipoUsuario;    
       try{
        bdSegura dbs=new bdSegura();
        Connection conex=dbs.conSeg();
        PreparedStatement pstmt=null;
        
        String consulta="select Id_usuario, Usuario, Clave, TipoUsuario FROM usuarios";
        pstmt=conex.prepareStatement(consulta);
        ResultSet rs= pstmt.executeQuery();
        
        String centinela="n";
         
         %>
         
           <table class="table">
                    <thead class="thead-light">
                        <tr>
                            <th scope="col">Id_usuario</th>
                            <th scope="col">Usuario</th>
                            <th scope="col">Clave</th>
                            <th scope="col">TipoUsuario</th>
                            <th scope="col" href="">Acciones</th>

                 </tr>
             </thead>
      
             <% 
             while(rs.next()){
                 
                 centinela="s";
                Id_usuario=rs.getString(1);
                Usuario=rs.getString(2);
                Clave=rs.getString(3);
                TipoUsuario=rs.getString(4);
             
             %>
             <tbody>
                 <tr>
                     <td><%=Id_usuario%></td>
