                 </tr>
             </thead>
             
      
             <% 
             while(rs.next()){
                 
                 centinela="s";
                id_cliente=rs.getString(1);
                nombre=rs.getString(2);
                identidad=rs.getString(3);
                telefono=rs.getString(4);
                fechaing=rs.getString(5);
                año=rs.getString(6);
                cond=rs.getString(7);
                correo=rs.getString(8);
                precio=rs.getString(9);
                comentario=rs.getString(10);
             
             %>
             <tbody>
                 <tr>
                     <td><%=id_cliente%></td>
                     <td><%=nombre%></td>
                     <td><%=identidad%></td>
                     <td><%=telefono%></td>
                     <td><%=fechaing%></td>
                     <td><%=año%></td>
                     <td><%=cond%></td>
                     <td><%=correo%></td>
                     <td><%=precio%></td>
                     <td><%=comentario%></td>
                     
                    
                     
            
                        
                 </tr>  
                 
                 </tbody>
                 
                 <%
                     }

                     %>
                     
                     </tbody>
         </table>
                  
