<section id="tabla_resultado">  <%
       
       String id_cliente,nombre,identidad,telefono,fechaing,año,cond,correo,precio,comentario;    
       try{
        bdSegura dbs=new bdSegura();
        Connection conex=dbs.conSeg();
        PreparedStatement pstmt=null;
        
        String consulta="select id_cliente, NombreCliente, identidad, telefono, fecha_ingreso, año, cond, correo, precio, comentario FROM clientes";
        pstmt=conex.prepareStatement(consulta);
        ResultSet rs= pstmt.executeQuery();
        
        String centinela="n";
         
         %>
         
                <br><br>
                    
                  <div class="banner-text">
                                 <h2>BITÁCORA DE GESTIONES</h2>
                                </div>
         
                <br>
            <div class="container">
                <div class="row"> </div>
                    
                         
                 
                
                
                      
                            <div class="text-center">
                                <div class="container">
  <div class="row">
    <div class="col-sm">
                                
           <table class="table">
                    <thead class="thead-light">
                        <tr>
                            <th scope="col">id_Cliente</th>
                            <th scope="col">Nombre</th>
                            <th scope="col">Identidad</th>     
                            <th scope="col">Telefono</th>
                             <th scope="col">Fecha_CotizaciÓn</th>
                             <th scope="col">Año</th>
                             <th scope="col">Condición de pago</th>
                             <th scope="col">Correo</th>
                             <th scope="col">Precio</th>
                             <th scope="col">Etapa</th>
                            <th scope="col" href="">Acciones</th>

                 </tr>
             </thead>
             
      
             <% 
             while(rs.next()){
                 
                 centinela="s";
                id_cliente=rs.getString(1);
                nombre=rs.getString(2);
                identidad=rs.getString(3);
                telefono=rs.getString(4);
                fechaing=rs.getString(5);
                año=rs.getString(6);
                cond=rs.getString(7);
                correo=rs.getString(8);
                precio=rs.getString(9);
                comentario=rs.getString(10);
             
             %>
             <tbody>
                 <tr>
                     <td><%=id_cliente%></td>
                     <td><%=nombre%></td>
                     <td><%=identidad%></td>
                     <td><%=telefono%></td>
                     <td><%=fechaing%></td>
                     <td><%=año%></td>
                     <td><%=cond%></td>
                     <td><%=correo%></td>
                     <td><%=precio%></td>
                     <td><%=comentario%></td>
                     
                     
                     
                     <td><a href="eliminarBitacora.jsp?pidus=<%=id_cliente%>"><input type="submit" name="eliminar" value="eliminar" class="btn btn-lg btn-danger btn-block"></a> </td>
                     
            
                        
                 </tr>  
                 
                 </tbody>
                 
                 <%
                     }

                     %>
                     
                     </tbody>
         </table>
                     
           <a href="crear.jsp"><button type="button" name="crear" value="Crear"class="btn btn-primary">Crear nueva gestión</button></a>
                   <a href="usuarioVendedor.jsp"><button type="button" name="usuarioVendedor" value="usuarioVendedor"class="btn btn-warning">Regresar Menu</button></a><br>
                   <td><br><a href="reporteBitacora.jsp"> <input type="submit" name="reporte" value="Mostrar Reporte" class="btn btn-lg btn-primary btn-block"></a> </td>
                   <br><br> <br><br>          
                  
                   
                   
             <%
                 
                 pstmt.close();
                 conex.close();
                
             } catch(Exception e){
             e.printStackTrace();
}
             
             
             %>
    </div>
  </div></div></div></div>
</section> 
    
             
